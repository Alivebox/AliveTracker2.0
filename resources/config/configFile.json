{

    "createViewPort": true,

    "viewsContainerPackage": "Framework.ux.view.container.SimpleViewsContainer",

    "localizationEnabled":true,

    "localeFilesBaseUrls":[
        "resources/locales/ext-lang-",
        "resources/locales/alivetracker-lang-",
        "resources/locales/errors-lang-"
    ],

    "errorCodesClass": "Locales.ErrorCodes",

    "views": [
        {
            "name": "loginPage",
            "package": "AliveTracker.view.authentication.Login",
            "default": true
        },
        {
            "name": "forgotPasswordPage",
            "package": "AliveTracker.view.authentication.ForgotPassword"
        },
        {
            "name": "registerPage",
            "package": "AliveTracker.view.authentication.Register"
        },
        {
            "name": "homePage",
            "package": "AliveTracker.view.home.Home",
            "permissions": [
                "viewHome"
            ]
        },
        {
            "name": "groupDetailPage",
            "package": "AliveTracker.view.group.GroupDetail",
            "permissions": [
                "viewGroupDetail"
            ]
        }
    ]

}